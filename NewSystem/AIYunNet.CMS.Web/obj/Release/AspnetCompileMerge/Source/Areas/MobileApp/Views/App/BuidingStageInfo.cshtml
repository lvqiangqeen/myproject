@model AIYunNet.CMS.Domain.Model.WebBuidingStages
<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport">
	<meta content="yes" name="apple-mobile-web-app-capable" />
	<meta content="black" name="apple-mobile-web-app-status-bar-style" />
	<meta content="telephone=no" name="format-detection" />
	<title>维修详情</title>
	<link rel="stylesheet" type="text/css" href="~/Content/mobileapp/css/base.css">
	<link rel="stylesheet" type="text/css" href="~/Content/mobileapp/css/common.css">
	<script type="text/javascript" src="~/Content/mobileapp/js/jquery.min.js"></script>
	<script src="~/Content/mobileapp/js/common.js"></script>
</head>

<body>
	<header class="header" id="header">
		<a href="javascript:history.go(-1)" target=_self class="back">返回</a>
		<h1>施工阶段详情</h1>
	</header>
	<!--header-end-->

	<div class="container" id="container">
		<div class="tip">温馨提示：想了解更多信息可以点击屏幕下方电话咨询</div>
		<div class="min-price">
			<div class="min-price-tit"><h2>@Model.StageName 水电工</h2></div>
			<div class="min-price-con">@Html.Raw(Model.StageContent) 测试数据<img src="~/Content/mobileapp/images/repair-img.jpg">
				<img src="~/Content/mobileapp/images/repair-img.jpg">
			</div>
		</div>

		<div class="min-price-bot" id="footer">
			<div class="min-price-tel"><a href="#">电话咨询</a></div>
			<div class="min-price-order"><a href="#">我要预约</a></div>
		</div>

	</div>
	<!--container-end-->
</body>
</html>
