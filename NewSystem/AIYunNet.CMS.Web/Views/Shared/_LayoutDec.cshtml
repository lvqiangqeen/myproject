<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>脉空间</title>
    @Html.Partial("_headCssJs")
    @RenderSection("head", required: false)
</head>
<body style="position: relative;">
    <div class="wrap_1180">
        
        @Html.Partial("_LayoutDecTopBar")
        <!--头部搜索-->
        <div class="top_search">
            <div class="index_logo">
                <a href="http://www.shejiben.com" title="美国知名风险投资机构投资的设计交易平台 - 设计本"><div class="logo"></div></a>

                <div id="search_case">
                    <form id="quicksearch" method="get" action="http://www.shejiben.com/search.php" target="_blank">
                        <input type="hidden" name="searchType" value="1" id="searchType">
                        <span id="search_span">
                            <div id="search_title">找图片　　</div><em class="arrow"></em>
                            <label id="s_zsj" style="display:none;"><a class="search_tags clickToSearch" href="javascript:;" data-id="2">找图集</a><a class="search_tags clickToSearch" href="javascript:;" data-id="10">找案例</a><a class="search_tags clickToSearch" href="javascript:;" data-id="7">找设计师</a><a class="search_tags clickToSearch" href="javascript:;" data-id="9">找问答</a><a class="search_tags clickToSearch" href="javascript:;" data-id="3">找模型　</a></label>
                        </span>
                        <input type="text" value="请输入搜索关键词" class="key" name="q" id="search_val">
                        <em class="icon_search"></em>
                        <input type="submit" value="搜索" class="submit">
                    </form>
                </div>
                <span class="or">或</span>
                <a href="http://www.shejiben.com/zb/?from=nav" target="_blank" class="edit">提交设计需求<em></em></a>
            </div>
        </div>
        <!--头部搜索end-->
        <p class="clear"></p>
        <!--主导航-->
        <div class="wrap wrap_top_menu">
            <div class="top_menu">
                <!--整站主导航-->
                <ul>
                    <li class="cur" id="homeindex"><a href="/index/homeindex" class="on onindex">首页</a></li>

                    <li class="cur" id="">
                        <a href="/case/DecCaseList?DecType=1">设计案例</a>
                        <div class="inner_menu">
                            <a href="/case/DecCaseList?DecType=1"  class="on onindex">精选家装</a>
                            <a href="/case/DecCaseList?DecType=2">精选工装</a>
                        </div>
                    </li>
                    <li>
                        <a href="/Imglist/DecImagelist">设计图库</a>
                        <div class="inner_menu">
                            <a href="/Imglist/DecImagelist?DecType=1">家装图库</a>
                            <a href="/Imglist/DecImagelist?DecType=2">工装图库</a>
                        </div>
                    </li>
                    <li><a href="/DecorationCompany/DecCompanyList">装修公司</a></li>
                    <li><a href="/workers/DecDesignerList">找设计师</a></li>
                    <li>
                        <a href="/workers/DecWorkLeaderList">找装修</a>
                        <div class="inner_menu">
                            <a href="/workers/DecWorkLeaderList">找工长</a>
                            <a href="/Workers/DecWorkerList">找工人</a>
                        </div>
                    </li>

                    <li style="display:none;">
                        <a href="http://www.shejiben.com/mingjiahui/">名家汇</a>
                        <div class="inner_menu">
                            <a href="http://www.shejiben.com/images/jiexi/">案例解析</a>
                            <a href="http://www.shejiben.com/zt/">人物专访</a>
                        </div>
                    </li>
                    <li style="display:none;"><a href="http://ask.shejiben.com/">有问必答</a></li>
                    <li class="last">
                        <a href="/Download/DownLoadList">下载专区</a>
                        @*<div class="inner_menu">
                            <a href="http://mx.shejiben.com/?from=nav">3DMAX模型下载</a>
                            <a href="http://www.shejiben.com/news/">设计资讯</a>
                            <a href="http://www.shejiben.com/renmai/?from=nav">人脉广场</a>
                            <a href="http://www.shejiben.com/space/">空间谈</a>
                        </div>*@
                    </li>
                </ul>
                <script>
                    $(function () {
                        $(".top_menu > ul > li > a").click(function () {
                            $.session.set('top_menu', 'value')
                        })
                    })
                </script>
                <!--整站主导航 End-->
                <!--首页面包屑导航-->
                @RenderSection("IndexNav", required: false)
                <!--首页面包屑导航 end-->

            </div>
        </div>
        <!--主导航-->
        @RenderBody()
        <!--友情链接-->
        @Html.Partial("_LayoutDecFriendLink")
    </div>
    <!--版权等-->
    @Html.Partial("_LayoutDecCopyRight")
    <!--版权等 End-->
    @RenderSection("Scripts", required: false)
</body>
</html>
