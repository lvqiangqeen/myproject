@using AIYunNet.CMS.Domain.Model
@using AIYunNet.CMS.BLL.Service
@using AIYunNet.CMS.Common.Utility
@{
    Layout = null;
    WebContentService bll = new WebContentService();
    List<WebFriendLink> friendLinkList = bll.GetFriendLinkList();
}

<div class="wrap wrap_link">
    <div class="link_content">
        <div class="link_column_name">
            <p class="cur">友情链接</p>
        </div>
        <div class="link_detail">
            <div class="link_list">
                @foreach (WebFriendLink link in friendLinkList)
                {
                    <a href="@link.LinkUrl" target="_blank">@link.Name</a>                   
                }
                <div class="clear"></div>
            </div>
        </div>
    </div>
</div>
